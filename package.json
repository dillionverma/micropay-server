{
  "name": "lightning-dalle2-server",
  "version": "1.0.2",
  "description": "Lightning Dalle2 Server",
  "main": "index.js",
  "scripts": {
    "start": "yarn build && NODE_ENV=production node dist/index.js",
    "build": "swc src -d dist",
    "dev": "nodemon",
    "test": "NODE_ENV=test nyc mocha",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "db:drop": "npx prisma migrate reset --skip-generate --skip-seed --force",
    "db:push": "npx prisma db push",
    "db:pull": "npx prisma db pull",
    "db:seed": "npx prisma db seed",
    "db:reset": "yarn db:drop && yarn db:push && yarn db:seed",
    "postdb:push": "dbml-renderer -i ./prisma/schema.dbml -o schema.svg",
    "postprisma:generate": "dbml-renderer -i ./prisma/schema.dbml -o schema.svg",
    "secrets:update": "NODE_ENV=test ts-node scripts/GithubCI.ts",
    "lint": "eslint --ignore-path .eslintignore -c .eslintrc.js .",
    "lint:fix": "eslint --ignore-path .eslintignore -c .eslintrc.js . --fix"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
