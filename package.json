{
  "name": "typescript-starter",
  "version": "1.0.2",
  "description": "Modern typescript starter boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "yarn build && NODE_ENV=production node -r dotenv-safe/config dist/index.js",
    "build": "swc src -d dist",
    "dev": "nodemon",
    "test": "NODE_ENV=test nyc mocha",
    "lint": "eslint --ignore-path .eslintignore -c .eslintrc.js .",
    "lint:fix": "eslint --ignore-path .eslintignore -c .eslintrc.js . --fix"
  },
  "keywords": [],
  "author": "Dillion Verma",
  "license": "MIT",
  "dependencies": {
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.182",
    "@swc/register": "^0.1.10",
    "@types/chai-as-promised": "^7.1.5",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "dotenv-safe": "^8.2.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^10.0.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.16",
    "nyc": "^15.1.0",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
}
